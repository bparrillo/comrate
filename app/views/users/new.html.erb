<h1>New User</h1>

<%= 'Sign up' %>

<%= form_for @user do |f| %>
<p>
  <%= f.label :username %> 
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :password %>
  <%= f.password_field :password %>
</p>
<p>
  <%= f.label :email %>
  <%= f.password_field :email %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>